!!! html
%html
  %head
    %title BigGym
    = stylesheet_link_tag    'application', media: 'all'
    = javascript_tag "API_SERVER = '#{request.base_url}';"
    = javascript_include_tag 'application'
    = csrf_meta_tags
  %body(ng-app="bigGym")
    = render 'header'
    = render 'breadcrumbs'
    = render 'error'
    = yield
    = render 'footer'
